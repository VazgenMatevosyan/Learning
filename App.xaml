<Application x:Class="UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UI"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Insert" UriSource="Icons\Insert.png"/>
            <BitmapImage x:Key="Remove" UriSource="Icons\Remove.png"/>
            <BitmapImage x:Key="Unenrol" UriSource="Icons\unenrol.png"/>
            <BitmapImage x:Key="View" UriSource="Icons\view.png"/>
            <BitmapImage x:Key="Back" UriSource="Icons\back.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
